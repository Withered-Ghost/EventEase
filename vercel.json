{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js",
            "methods" : ["GET","POST"],
            "headers" : { "key": "Access-Control-Allow-Origin", "value": "*" }
        }
    ]
}